# Docker Environment Configuration
# Copy this to .env for Docker Compose setup

# API Configuration - Docker internal networking
# Use 'backend' as hostname when backend is in same docker-compose network
VITE_API_BASE_URL=http://backend:9090/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_JWT_SECRET=docker-jwt-secret-change-in-production

# Application
VITE_APP_NAME=AI E-Commerce (Docker)
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false

# Features
VITE_ENABLE_AI_RECOMMENDATIONS=true
VITE_ENABLE_ANALYTICS=false

# Environment
NODE_ENV=development
